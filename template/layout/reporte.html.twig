<!DOCTYPE html>
<html lang="en">
    <head>
        <meta charset="utf-8">
        <title>Reporte - {% block titulo '' %} - {{ constant('TITULO') }}</title>
        <meta name="viewport" content="width=device-width, initial-scale=1.0">
        <meta name="description" content="interlink">
        <meta name="author" content="Anyul Rivas">
        <!-- Le styles -->
        <link href="{{constant('URL_SISTEMA')}}css/bootstrap.css" rel="stylesheet">
        <link href="{{constant('URL_SISTEMA')}}css/style.css" rel="stylesheet">
        <link href="{{constant('URL_SISTEMA')}}css/bootstrap-responsive.css" rel="stylesheet">
        <!-- Le HTML5 shim, for IE6-8 support of HTML5 elements -->
        <!--[if lt IE 9]>
          <script src="http://html5shim.googlecode.com/svn/trunk/html5.js"></script>
        <![endif]-->
        </head>
    <body>
            <div class="container-fluid">
                <div class="row-fluid">
                    <!-- content -->
                    <div class="span12">
                        {% block contenido %}
                            <table class="table">
                                <thead>
                                    <tr>
                                        <th>Tabla de prueba</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <tr>
                                        <td>Datos de prueba</td>
                                    </tr>
                                </tbody>
                            </table>
                        {% endblock %}
                    </div><!--/span-->
                </div><!--/row-->
            <hr>
            <footer>
                <p>Interlink &copy; {{'now'|date('Y')}}</p>
            </footer>
        </div><!--/.fluid-container-->
    </body>
</html>
