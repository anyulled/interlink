{% if funcionalidades|length> 0 %}
<div class="well sidebar-nav">
    <ul class="nav nav-list">
        <li class="nav-header">Administrativo</li>
        {% for funcionalidad in funcionalidades if funcionalidad.GRUPO == "administracion" %}
        <li><a href="{{ constant('URL_SISTEMA') ~ funcionalidad.URL }} " title="{{ funcionalidad.DESCRIPCION|capitalize }}">{{ funcionalidad.NOMBRE|title }}</a></li>
        {% endfor %}
        <li class="nav-header">Usuario</li>
        {% for funcionalidad in funcionalidades if funcionalidad.GRUPO == "usuario" %}
        <li><a href="{{ constant('URL_SISTEMA') ~ funcionalidad.URL }} " title="{{ funcionalidad.DESCRIPCION|capitalize }}">{{ funcionalidad.NOMBRE|title }}</a></li>
        {% endfor %}
        <li class="nav-header">Archivo</li>
        {% for funcionalidad in funcionalidades if funcionalidad.GRUPO == "archivo" %}
        <li><a href="{{ constant('URL_SISTEMA') ~ funcionalidad.URL }} " title="{{ funcionalidad.DESCRIPCION|capitalize }}">{{ funcionalidad.NOMBRE|title }}</a></li>
        {% endfor %}
        <li class="nav-header">Reportes</li>
        {% for funcionalidad in funcionalidades if funcionalidad.GRUPO == "reporte" %}
        <li><a href="{{ constant('URL_SISTEMA') ~ funcionalidad.URL }} " title="{{ funcionalidad.DESCRIPCION|capitalize }}">{{ funcionalidad.NOMBRE|title }}</a></li>
        {% endfor %}
    </ul>
</div>
{% endif %}